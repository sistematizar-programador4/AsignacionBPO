{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard"
  ],
  "linterOptions": {
    "exclude": [
        "node_modules/**"
    ]
},
  "jsRules": {},
  "rules": {
  "no-console": false,
    "trailing-comma": false,
    "indent": [true, "spaces", 2],
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "await-promise": false
  },
  "rulesDirectory": []
}